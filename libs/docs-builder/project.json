{
  "name": "docs-builder",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/docs-builder/src",
  "projectType": "library",
  "tags": ["skip-ci", "type:lib", "scope:lib"],
  "targets": {
    "check-mirror-exists": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "echo 'docs-builder is internal - skipping mirror check'"
          }
        ]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "commands": [
          {
            "command": "bun run build"
          }
        ],
        "cwd": "libs/docs-builder",
        "parallel": false
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run dev"
          }
        ],
        "cwd": "libs/docs-builder",
        "parallel": false
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run preview"
          }
        ],
        "cwd": "libs/docs-builder",
        "parallel": false
      }
    },
    "verify": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run verify"
          }
        ],
        "cwd": "libs/docs-builder",
        "parallel": false
      }
    }
  }
}
