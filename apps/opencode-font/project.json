{
  "name": "opencode-font",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/opencode-font/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run build"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run dev"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run test"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run test:coverage"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "generate-fonts": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run generate:fonts"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "validate-fonts": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run validate:fonts"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run lint"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run format"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "bun run typecheck"
          }
        ],
        "cwd": "apps/opencode-font",
        "parallel": false
      }
    }
  }
}
